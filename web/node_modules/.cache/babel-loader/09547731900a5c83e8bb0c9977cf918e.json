n: 'left',\n        form: {\n          component: {\n            props: {\n              elProps: { // 与el-uploader 配置一致\n                multiple: false,\n                limit: 5 // 限制5个文件\n              },\n              sizeLimit: 50 * 1024 // 不能超过限制\n            },\n            span: 24\n          },\n          helper: '限制文件大小不能超过50k'\n        }\n      },\n      {\n        title: '多选,本地,自动染色',\n        key: 'select2',\n        sortable: true,\n        width: 180,\n        search: {\n          disabled: false,\n          title: '多选'\n        },\n        type: 'select',\n        form: {\n          title: '多选本地',\n          component: {\n            props: {\n              filterable: true,\n              multiple: true,\n              clearable: true\n            }\n          }\n        },\n        dict: {\n          data: [{ value: 'sz', label: '深圳' }, { value: 'gz', label: '广州' }, { value: 'wh', label: '武汉' }, { value: 'sh', label: '上海' }]\n        },\n        component: { props: { color: 'auto' } } // 自动染色\n      }\n    ]\n  }\n}\n"],"mappings":"AAAA,SAASA,OAAO,QAAQ,eAAe;AACvC,OAAO,IAAMC,WAAW,GAAG,SAAdA,WAAW,CAAIC,EAAE,EAAK;EACjC,OAAO;IACLC,WAAW,EAAE;MACXC,OAAO,EAAE;IACX,CAAC;IACDC,OAAO,EAAE;MACPC,MAAM,EAAE;IACV,CAAC;IACDC,WAAW,EAAE;MACXC,aAAa,EAAE;IACjB,CAAC;IACDC,WAAW,EAAE;MACXC,WAAW,EAAE,EAAE,CAAC;IAClB,CAAC;;IACDC,OAAO,EAAE,CACP;MACEC,KAAK,EAAE,IAAI;MACXC,GAAG,EAAE,IAAI;MACTC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE;QACJC,QAAQ,EAAE;MACZ;IACF,CAAC,EACD;MACEJ,KAAK,EAAE,MAAM;MACbC,GAAG,EAAE,SAAS;MACdI,QAAQ,EAAE,IAAI;MACdC,MAAM,EAAE;QACNF,QAAQ,EAAE;MACZ,CAAC;MACDG,IAAI,EAAE,gBAAgB;MACtBC,IAAI,EAAE;QACJC,GAAG,EAAE,mBAAmB;QACxBC,KAAK,EAAE,IAAI;QAAE;QACbC,KAAK,EAAE,MAAM;QAAE;QACfC,OAAO,EAAE,iBAACH,GAAG,EAAED,IAAI,QAA0B;UAAA,IAAtBL,IAAI,QAAJA,IAAI;YAAEU,SAAS,QAATA,SAAS;UACpC,OAAOzB,OAAO,CAAC;YAAEqB,GAAG,EAAEA,GAAG;YAAEK,MAAM,EAAE;cAAEC,IAAI,EAAE,CAAC;cAAEC,KAAK,EAAE;YAAE;UAAE,CAAC,CAAC,CAACC,IAAI,CAAC,UAAAC,GAAG,EAAI;YACtEL,SAAS,CAACM,QAAQ,CAACJ,IAAI,GAAGG,GAAG,CAACE,IAAI,C