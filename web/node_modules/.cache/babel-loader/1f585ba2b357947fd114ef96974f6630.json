 }/>\n  else if (menu.iconSvg) icon = <d2-icon-svg slot=\"title\" name={ menu.iconSvg }/>\n  else icon = <i slot=\"title\" class=\"fa fa-folder-o\"/>\n  return <el-submenu\n    key={ menu.path }\n    index={ menu.path }>\n    { icon }\n    <span slot=\"title\">{ menu.title || '未命名菜单' }</span>\n    { menu.children.map(child => createMenu.call(this, h, child)) }\n  </el-submenu>\n}\n\n/**\n * @description 在组件中调用此方法渲染菜单项目\n * @param {Function} h createElement\n * @param {Object} menu 菜单项\n */\nexport function createMenu (h, menu) {\n  if (menu.children === undefined) return elMenuItem.call(this, h, menu)\n  return elSubmenu.call(this, h, menu)\n}\n"],"mappings":";AAAA;AACA;AACA;AACA;AACA;AACA,OAAO,SAASA,UAAU,CAAEC,CAAC,EAAEC,IAAI,EAAE;EACnC,IAAIC,IAAI,GAAG,IAAI;EACf,IAAID,IAAI,CAACC,IAAI,EAAEA,IAAI;IAAA,yBAAuBD,IAAI,CAACC,IAAI;EAAA,EAAM,MACpD,IAAID,IAAI,CAACE,OAAO,EAAED,IAAI;IAAA;MAAA,QAAuBD,IAAI,CAACE;IAAO;EAAA,EAAI,MAC7DD,IAAI;IAAA,SAAY;EAAc,EAAE;EACrC;IAAA,OACQD,IAAI,CAACG,IAAI;IAAA;MAAA,SACPH,IAAI,CAACG;IAAI;EAAA,IACfF,IAAI;IAAA,QACK;EAAO,IAAGD,IAAI,CAACI,KAAK,IAAI,OAAO;AAE9C;;AAEA;AACA;AACA;AACA;AACA;AACA,OAAO,SAASC,SAAS,CAAEN,CAAC,EAAEC,IAAI,EAAE;EAAA;EAClC,IAAIC,IAAI,GAAG,IAAI;EACf,IAAID,IAAI,CAACC,IAAI,EAAEA,IAAI;IAAA,QAAW,OAAO;IAAA,yBAAkBD,IAAI,CAACC,IAAI;EAAA,EAAM,MACjE,IAAID,IAAI,CAACE,OAAO,EAAED,IAAI;IAAA,QAAqB,OAAO;IAAA;MAAA,QAAQD,IAAI,CAACE;IAAO;EAAA,EAAI,MAC1ED,IAAI;IAAA,QAAW,OAAO;IAAA,SAAO;EAAgB,EAAE;EACpD;IAAA,OACQD,IAAI,CAACG,IAAI;IAAA;MAAA,SACPH,IAAI,CAACG;IAAI;EAAA,IACfF,IAAI;IAAA,QACK;EAAO,IAAGD,IAAI,CAACI,KAAK,IAAI,OAAO,IACxCJ,IAAI,CAACM,QAAQ,CAACC,GAAG,CAAC,UAAAC,KAAK;IAAA,OAAIC,UAAU,CAACC,IAAI,CAAC,KAAI,EAAEX,CAAC,EAAES,KAAK,CAAC;EAAA,EAAC;AAEjE;;AAEA;AACA;AACA;AACA;AACA;AACA,OAAO,SAASC,UAAU,CAAEV,CAAC,EAAEC,IAAI,EAAE;EACnC,IAAIA,IAAI,CAACM,QAAQ,KAAKK,SAAS,EAAE,OAAOb,UAAU,CAACY,IAAI,CAAC,IAAI,EAAEX,CAAC,EAAEC,IAAI,CAAC;EACtE,OAAOK,SAAS,CAACK,IAAI,CAAC,IAAI,EAAEX,CAAC,EAAEC,IAAI,CAAC;AACtC"}]}