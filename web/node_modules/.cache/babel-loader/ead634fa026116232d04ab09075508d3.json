  },\n    /**\n     * @description 本地加载配置\n     * @param {Object} context\n     */\n    async load ({ state, dispatch, commit }) {\n      // store 赋值\n      const data = await dispatch('d2admin/db/get', {\n        dbName: 'sys',\n        path: 'settings.init',\n        defaultValue: {},\n        user: true\n      }, { root: true })\n      commit('set', data)\n    }\n  },\n  mutations: {\n    /**\n     * @description 获取配置\n     * @param {Object} state state\n     * @param {String} key active\n     * @param {Object} value active\n     */\n    async get (state, key, value) {\n      return state[key]\n    },\n    /**\n     * @description 赋值系统配置\n     * @param {Object} state state\n     * @param {Object} value active\n     */\n    async set (state, value) {\n      state.data = value\n      state.keepRecord = value['login.keep_record']\n      return state.data\n    }\n  }\n}\n"],"mappings":";;AAAA,SAASA,OAAO,QAAQ,eAAe;AAEvC,OAAO,IAAMC,SAAS,GAAG,qBAAqB;;AAE9C;AACA,eAAe;EACbC,UAAU,EAAE,IAAI;EAChBC,KAAK,EAAE;IACLC,IAAI,EAAE,CAAC;EACT,CAAC;EACDC,OAAO,EAAE;IACP;AACJ;AACA;AACA;IACUC,IAAI,sBAA+B;MAAA;QAAA;QAAA;UAAA;YAAA;cAA3BH,KAAK,QAALA,KAAK,EAAEI,QAAQ,QAARA,QAAQ,EAAEC,MAAM,QAANA,MAAM;cACnC;cACAR,OAAO,CAAC;gBACNS,GAAG,EAAER,SAAS;gBACdS,MAAM,EAAE;cACV,CAAC,CAAC,CAACC,IAAI;gBAAA,uEAAC,iBAAMC,GAAG;kBAAA;oBAAA;sBAAA;wBAAA;wBAAA,OAETL,QAAQ,CAAC,gBAAgB,EAAE;0BAC/BM,MAAM,EAAE,KAAK;0BACbC,IAAI,EAAE,eAAe;0BACrBC,KAAK,EAAEH,GAAG,CAACR,IAAI;0BACfY,IAAI,EAAE;wBACR,CAAC,EAAE;0BAAEC,IAAI,EAAE;wBAAK,CAAC,CAAC;sBAAA;wBAClBV,QAAQ,CAAC,MAAM,CAAC;sBAAA;sBAAA;wBAAA;oBAAA;kBAAA;gBAAA,CACjB;gBAAA;kBAAA;gBAAA;cAAA,IAAC;YAAA;YAAA;cAAA;UAAA;QAAA;MAAA;IACJ,CAAC;IACD;AACJ;AACA;AACA;IACUW,IAAI,uBAA+B;MAAA;QAAA;QAAA;UAAA;YAAA;cAA3Bf,KAAK,SAALA,KAAK,EAAEI,QAAQ,SAARA,QAAQ,EAAEC,MAAM,SAANA,MAAM;cAAA;cAAA,OAEhBD,QAAQ,CAAC,gBAAgB,EAAE;gBAC5CM,MAAM,EAAE,KAAK;gBACbC,IAAI,EAAE,eAAe;gBACrBK,YAAY,EAAE,CAAC,CAAC;gBAChBH,IAAI,EAAE;cACR,CAAC,EAAE;gBAAEC,IAAI,EAAE;cAAK,CAAC,CAAC;YAAA;cALZb,IAAI;cAMVI,MAAM,CAAC,KAAK,EAAE