active = await dispatch('d2admin/db/get', {\n        dbName: 'sys',\n        path: 'transition.active',\n        defaultValue: setting.transition.active,\n        user: true\n      }, { root: true })\n    }\n  }\n}\n"],"mappings":";;AAAA;AACA,OAAOA,OAAO,MAAM,cAAc;AAElC,eAAe;EACbC,UAAU,EAAE,IAAI;EAChBC,KAAK,EAAE;IACL;IACAC,MAAM,EAAEH,OAAO,CAACI,UAAU,CAACD;EAC7B,CAAC;EACDE,OAAO,EAAE;IACP;AACJ;AACA;AACA;AACA;IACUC,GAAG,qBAAuBH,MAAM,EAAE;MAAA;QAAA;QAAA;UAAA;YAAA;cAA3BD,KAAK,QAALA,KAAK,EAAEK,QAAQ,QAARA,QAAQ;cAC1B;cACAL,KAAK,CAACC,MAAM,GAAGA,MAAM;cACrB;cAAA;cAAA,OACMI,QAAQ,CAAC,gBAAgB,EAAE;gBAC/BC,MAAM,EAAE,KAAK;gBACbC,IAAI,EAAE,mBAAmB;gBACzBC,KAAK,EAAER,KAAK,CAACC,MAAM;gBACnBQ,IAAI,EAAE;cACR,CAAC,EAAE;gBAAEC,IAAI,EAAE;cAAK,CAAC,CAAC;YAAA;YAAA;cAAA;UAAA;QAAA;MAAA;IACpB,CAAC;IACD;AACJ;AACA;AACA;IACUC,IAAI,uBAAuB;MAAA;QAAA;QAAA;UAAA;YAAA;cAAnBX,KAAK,SAALA,KAAK,EAAEK,QAAQ,SAARA,QAAQ;cAAA;cAAA,OAENA,QAAQ,CAAC,gBAAgB,EAAE;gBAC9CC,MAAM,EAAE,KAAK;gBACbC,IAAI,EAAE,mBAAmB;gBACzBK,YAAY,EAAEd,OAAO,CAACI,UAAU,CAACD,MAAM;gBACvCQ,IAAI,EAAE;cACR,CAAC,EAAE;gBAAEC,IAAI,EAAE;cAAK,CAAC,CAAC;YAAA;cALlBV,KAAK,CAACC,MAAM;YAAA;YAAA;cAAA;UAAA;QAAA;MAAA;IAMd;EACF;AACF,CAAC"}]}