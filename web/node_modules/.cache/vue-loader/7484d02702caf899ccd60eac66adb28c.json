.$parent.valueBinding\n        this.setValue(row[valueBinding])\n        this.key = children\n      } else {\n        this.setValue([])\n      }\n    }\n  },\n  created () {\n    const { row } = this.$parent.scope\n    const { children } = this.$parent\n    if (children) {\n      const valueBinding = this.$parent.valueBinding\n      this.setValue(row[valueBinding])\n      this.key = children\n    } else {\n      this.setValue([])\n    }\n  },\n  methods: {\n    setValue (value) {\n      // 在这里对 传入的value值做处理\n      this.currentValue = value\n    }\n  }\n}\n</script>\n"]}]}