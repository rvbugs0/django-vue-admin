efault {\n  name: 'd2-contextmenu-list',\n  props: {\n    menulist: {\n      type: Array,\n      default: () => []\n    }\n  },\n  methods: {\n    rowClick (event) {\n      let target = event.target\n      while (!target.dataset.value) {\n        target = target.parentNode\n      }\n      this.$emit('rowClick', target.dataset.value)\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n.d2-contentmenu-list {\n  .d2-contentmenu-item {\n    padding: 8px 20px 8px 15px;\n    margin: 0;\n    font-size: 14px;\n    color: #606266;\n    cursor: pointer;\n    &:hover {\n      background: #ecf5ff;\n      color: #66b1ff;\n    }\n    .d2-contentmenu-item-title {\n      margin-left: 10px;\n    }\n  }\n}\n</style>\n"]}]}