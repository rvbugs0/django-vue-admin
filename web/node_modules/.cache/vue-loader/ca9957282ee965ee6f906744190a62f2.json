a () {\n    return {\n      form: {\n        title: null,\n        key: null\n      },\n      rules: {\n        title: [\n          {\n            required: true,\n            message: '请输入'\n          }\n        ],\n        key: [\n          {\n            required: true,\n            message: '请输入'\n          },\n          {\n            pattern: /^[A-Za-z0-9]+$/,\n            message: '只能是英文和数字'\n          }\n        ]\n      }\n    }\n  },\n  methods: {\n    onSubmit () {\n      const that = this\n      that.$refs.form.validate((valid) => {\n        if (valid) {\n          api.createObj(that.form).then(res => {\n            this.$message.success('新增成功')\n            this.refreshView()\n          })\n        } else {\n          console.log('error submit!!')\n          return false\n        }\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>\n"]}]}